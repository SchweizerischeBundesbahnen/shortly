        <em:description>Encurtador rápido de endereços web.</em:description>
      </Description>
    </em:localized>

    <em:localized>
      <Description>
        <em:locale>de</em:locale>
        <em:name>Shortly URL-Verkürzer</em:name>
        <em:creator>aloshbennett</em:creator>
        <em:translator>Sierk Bornemann (sierkb)</em:translator>
        <em:description>1-Klick-URL-Verkürzer</em:description>
      </Description>
    </em:localized>
